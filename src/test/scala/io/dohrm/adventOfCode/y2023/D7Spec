package io.dohrm.adventOfCode.y2023

class D7Spec extends munit.FunSuite {
  val sample: String =
    """32T3K 765
      |T55J5 684
      |KK677 28
      |KTJJT 220
      |QQQJA 483""".stripMargin

  test("part1") {
    assertEquals(D7.part1(sample), 6440L)
  }

  test("part2") {
    assertEquals(D7.part2(sample), 5905L)
  }
}
